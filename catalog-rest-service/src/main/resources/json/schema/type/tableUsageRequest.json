{
  "$id": "https://open-metadata.org/schema/entity/data/tableUsageRequest.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Table Usage Request",
  "description": "This schema defines type of table usage request",
  "javaType": "org.openmetadata.catalog.type.TableUsageRequest",
  "type": "object",
  "properties": {
    "date": {
      "description": "Date of execution of SQL query",
      "type": "string"
    },
    "count": {
      "description": "Usage count of table",
      "type": "integer"
    }
  },
  "required": [
    "sql",
    "serviceName",
    "tables",
    "columns",
    "tableAliases",
    "database"
  ],
  "additionalProperties": false
}
