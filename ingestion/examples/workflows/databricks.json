{
    "source": {
      "type": "databricks",
      "config": {
        "token": "<databricks token>",
        "host_port": "host:port",
        "database": "default",
        "service_name": "local_databricks",
        "connect_args":{
          "http_path": "http_path"
        },
        "table_filter_pattern": {
          "excludes": []
        }
      }
    },
    "sink": {
      "type": "metadata-rest",
      "config": {}
    },
    "metadata_server": {
      "type": "metadata-server",
      "config": {
        "api_endpoint": "http://localhost:8585/api",
        "auth_provider_type": "no-auth" 
      }
    }
  }
  