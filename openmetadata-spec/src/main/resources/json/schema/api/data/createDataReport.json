{
  "$id": "https://open-metadata.org/schema/api/data/createDataReport.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CreateDataReport",
  "description": "This schema defines data report from OpenMetadata",
  "type": "object",
  "javaType": "org.openmetadata.schema.api.data.CreateDataReport",
  "javaInterfaces": ["org.openmetadata.schema.CreateEntity"],
  "properties": {
    "name": {
      "description": "Unique name of data report",
      "$ref": "../../type/basic.json#/definitions/entityName"
    },
    "displayName": {
      "description": "Display Name that identifies this data report.",
      "type": "string"
    },
    "description": {
      "description": "Description of the data report",
      "$ref": "../../type/basic.json#/definitions/markdown"
    },
    "dataReportType": {
      "description": "Type of DataReport - dataInsight",
      "$ref": "../../entity/data/dataReport.json#/definitions/dataReportType"
    },
    "endpointType": {
      "description": "Define the type via which the data report to be sent.",
      "type": "string"
    },
    "endpointConfiguration": {
      "description": "Endpoint Config",
      "$ref": "../../email/emailConfig.json"
    },
    "scheduleConfig": {
      "description": "Scheduler Interval for the data report in cron format.",
      "type": "string"
    }
  },
  "required": ["name", "dataReportType", "scheduleConfig", "endpointType", "endpointConfiguration"],
  "additionalProperties": false
}